{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "ie >= 11"
                    ]
                },
                "exclude": [
                    "transform-async-to-generator",
                    "transform-regenerator"
                ],
                "modules": false,
                "loose": true
            }
        ],
        "jest"
    ],
    "plugins": [
        "transform-object-rest-spread"
    ],
    "env": {
        "commonjs": {
            "presets": [
                [
                    "env",
                    {
                        "loose": true
                    }
                ]
            ]
        },
        "test": {
            "presets": [
              [
                "env",
                {
                  "modules": "commonjs",
                  "loose": true
                }
              ]
            ]
        }
    }
}